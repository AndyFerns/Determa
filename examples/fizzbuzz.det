// FizzBuzz in Determa
// Prints numbers 1 to 15
// - Multiples of 3: "Fizz"
// - Multiples of 5: "Buzz"
// - Multiples of both: "FizzBuzz"

var i = 1;
var limit = 15;

while i <= limit {
    // Check for both 3 and 5 first (15)
    if i % 15 == 0 {
        print "FizzBuzz";
    } 
    elif i % 3 == 0 {
        print "Fizz";
    }
    elif i % 5 == 0 {
        print "Buzz";
    }
    else {
        print i;
    }

    // Compound Assignment!
    i += 1; 
}